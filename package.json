{
  "name": "cheatsheet-generate",
  "private": true,
  "version": "0.0.61",
  "description": "AI-based cheatsheet generator and downloader",
  "scripts": {
    "download": "node utils/codepen-downloader/codepen-downloader.js",
    "download:dry": "node utils/codepen-downloader/codepen-downloader.js --dry-run",
    "test:finalhtml": "node utils/codepen-downloader/test-finalhtml.js",
    "build": "node scripts/generate-nav.js && node scripts/inject-favicon.js",
    "meta:migrate": "node scripts/migrate-meta.js",
    "meta:validate": "node scripts/validate-meta.js",
    "meta:inject": "node scripts/inject-meta-into-html.js",
    "fav:inject": "node scripts/inject-favicon.js",
    "watch": "node scripts/watch.js",
    "workflow": "node scripts/workflow.js",
    "release": "npm run release:patch",
    "release:patch": "node scripts/workflow.js --bump patch",
    "release:minor": "node scripts/workflow.js --bump minor",
    "release:major": "node scripts/workflow.js --bump major"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.5",
    "he": "^1.2.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.4",
    "js-yaml": "^4.1.1",
    "node-fetch": "^2.6.9",
    "sharp": "^0.34.5"
  },
  "engines": {
    "node": ">=16"
  }
}
